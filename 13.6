#include <stdio.h>
#include <math.h>
int *roots(int*ptr1,int*roots)
{int r1,r2,a,b,c;
a=*(ptr1);
b=*(ptr1+1);
c=*(ptr1+2);
r1=(-b+sqrt(b*b-4*a*c))/2*a;
r2=(-b-sqrt(b*b-4*a*c))/2*a;
roots[0]=r1;
roots[1]=r2;
return(roots);
}
int main()
{int c[3]={1,-5,6};
int r[2];
printf("%d,%d",*(roots(c,r)),*(roots(c,r)+1));
return 0;
}
